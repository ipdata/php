{
    "name": "ipdata/api-client",
    "type": "library",
    "description": "API client for ipdata.co",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "php": ">= 7.2",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.2",
        "php-http/discovery": "^1.7",
        "php-http/httplug": "^2.0",
        "php-http/mock-client": "^1.3",
        "phpunit/phpunit": "^8.5",
        "symfony/http-client": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Ipdata\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ipdata\\ApiClient\\Tests\\": "tests/"
        }
    }
}
